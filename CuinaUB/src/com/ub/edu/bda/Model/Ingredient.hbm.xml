<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package = "com.ub.edu.bda.Model">
    <class name="com.ub.edu.bda.Model.Ingredient" table="INGREDIENT">
        <id column="ID" name="id_Ingredient">
            <generator class="identity"/>
        </id>
        <property column="NOM" name="nom"/> 
        <property column="FAMILIA" name="familia"/> 
        <property column="REFRIGERACIO" name="refrigeracio"/> 
        

    </class>
</hibernate-mapping>