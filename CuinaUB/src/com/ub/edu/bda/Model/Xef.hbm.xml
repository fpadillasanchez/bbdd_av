<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package = "com.ub.edu.bda.Model">
    <class name="com.ub.edu.bda.Model.Xef" table="XEF">
        <id column="ID" name="id_Xef">
            <generator class="identity"/>
        </id>
        <property column="NOM" name="nom"/>
        <property column="ESTRELLES" name="int_Estrelles"/>
        
        <set name="receptes" >
            <key column="ID"/>
            <one-to-many class ="com.ub.edu.bda.Model.Recepta"/>
        </set>
    
        

    </class>
</hibernate-mapping>