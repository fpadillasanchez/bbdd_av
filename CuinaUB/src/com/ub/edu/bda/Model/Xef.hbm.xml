<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ub.edu.bda.Model.Xef" table="XEF">
    <id column="ID" name="id_Xef">
      <generator class="identity"/>
    </id>
    <property column="NOM" name="nom"/>
    <property column="ESTRELLES" name="int_Estrelles"/>
        
         <one-to-many name="catalogo" class="com.ub.edu.bda.Model.Catalogo" fetch="select">
           <column name="ID_CATALOGO" /> <!-- CAMP FK de la taula articulo IDENTICS A LES DUES ENTITATS --> 
       </one-to-many>
    </class>
</hibernate-mapping>